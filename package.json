{
  "name": "vrmviewmeister",
  "author": {
    "name": "ISHII Eiju"
  },
  "description": "Web app for VRoid(VRM) : Posing, Animation",
  "version": "2.16.0",
  "engines": {
    "node" : ">=22.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=delevopment&&set PORT=5026&&node ./app.js",
    "start": "set NODE_ENV=production&&node ./app.js",
    "ssldev": "browser-sync start --no-open --config bs-config.js",
    "mainte:largefile": "node z_commit/build_largefile.js",
    "mainte:upload": "node z_commit/azure-blobup.js",
    "prepare1:ps": "powershell .\\z_mng\\download_largefile.ps1",
    "prepare1:sh": "./z_mng/download_largefile.sh",
    "prepare2:ps": "powershell .\\z_mng\\extract_largefile.ps1",
    "prepare2:sh": "./z_mng/extract_largefile.sh",
    "fullbuild": "webpack&workbox generateSW workbox-config.js",
    "wpbuild": "webpack",
    "watch": "webpack --watch",
    "swbuild": "workbox generateSW workbox-config.js",
    "compile:electron": "node z_mng/compile_electron4html.js",
    "electron": "electron .",
    "build:win": "electron-builder --win --x64",
    "build:win-portable": "electron-builder --win --x64 --dir",
    "build:mac": "electron-builder --mac --x64",
    "build:linux": "electron-builder --linux --x64",
    "cleanup:ps": "powershell .\\z_mng\\cleanup_largefile.ps1",
    "cleanup:sh": "./z_mng/cleanup_largefile.sh"
  },
  "main": "main.js",
  "build": {
    "appId": "com.simazir.vrmviewmeister",
    "afterSign": "notarize.js",
    "directories": {
      "output": "dist"
    },
    "files": [
      "assets",
      "public",
      "main.js",
      "routes/azure-lib.js",
      "preload.js",
      "ipc01vrh.js",
      "ipc02azur.js",
      "vrh.env",
      "package.json",
      "package-lock.json"
    ],
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "dist/entitlements.mac.plist",
      "entitlementsInherit": "dist/entitlements.mac.plist",
      "icon": "assets/app_icon.icns",
      "target": [
        "dmg"
      ],
      "category": "public.app-category.graphics-design"
    },
    "win": {
      "icon": "assets/app_icon.ico",
      "target": "nsis"
    },
    "linux": {
      "icon": "assets/app_icon.png",
      "target": "AppImage",
      "category": "Graphics"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "icon": "assets/app_icon.icns",
      "sign": false
    },
    "appImage": {
      "category": "Graphics",
      "desktop": {
        "Name": "VRMViewMeister",
        "Comment": "",
        "Icon": "assets/app_icon.png",
        "Type": "Application",
        "Terminal": false,
        "Encoding": "UTF-8",
        "Categories": "Graphics"
      }
    }
  },
  "dependencies": {
    "@azure/storage-blob": "^12.26.0",
    "archiver": "^7.0.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cookie-parser": "~1.4.7",
    "csrf-csrf": "^3.1.0",
    "debug": "~4.4.0",
    "dotenv": "^16.4.7",
    "electron-notarize": "^1.2.2",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "morgan": "~1.10.0",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "workbox-cli": "^7.3.0"
  },
  "devDependencies": {
    "@interactjs/types": "^1.10.27",
    "@intlify/vue-i18n-loader": "^5.0.1",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "interactjs": "^1.10.27",
    "quasar": "^2.17.7",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "workbox-sw": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  }
}
